<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_page"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="407dp"
        android:layout_height="31dp"
        android:layout_marginTop="20dp"
        android:text="WELCOME TO HUB"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="410dp"
        android:layout_height="303dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.057"
        app:srcCompat="@drawable/imm" />

    <Button
        android:id="@+id/addp"
        android:layout_width="148dp"
        android:layout_height="52dp"
        android:layout_marginLeft="5dp"
        android:background="@color/login_forms_details_light"
        android:drawableLeft="@drawable/ic_baseline_add_card_24"
        android:text="add property"
        android:textColor="@color/yellow"
        android:textSize="13dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/viewp"
        android:layout_width="139dp"
        android:layout_height="52dp"
        android:layout_marginRight="5dp"
        android:background="@color/login_forms_details_light"
        android:drawableLeft="@drawable/ic_baseline_remove_red_eye_24"
        android:text="view properties"
        android:textColor="@color/yellow"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile" />

    <Button
        android:id="@+id/profile"
        android:layout_width="103dp"
        android:layout_height="52dp"
        android:background="@color/login_forms_details_light"
        android:drawableLeft="@drawable/ic_baseline_cottage_24"
        android:text="PROFILE"
        android:textColor="@color/yellow"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/viewp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/addp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/forward_btn"
        android:layout_width="54dp"
        android:layout_height="58dp"
        android:layout_marginEnd="16dp"
        android:background="@color/login_forms_details_light"
        android:drawableLeft="@drawable/ic_baseline_login_24"
        app:layout_constraintBottom_toTopOf="@+id/addp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.286" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="243dp"
        android:layout_height="42dp"
        android:text="Create New Account -->"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/addp"
        app:layout_constraintEnd_toStartOf="@+id/forward_btn"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.332" />


</androidx.constraintlayout.widget.ConstraintLayout>